{
    "terminal.integrated.profiles.linux": {
        "kas": {
            "path": "bash",
            "args": ["--init-file", "scripts/videology-setup-env"],
            "icon": "terminal-debian"
        },
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        }
    },
    "terminal.integrated.defaultProfile.linux": "kas",
    "search.exclude": {
        "**/.repo": true,
        "**/build/**": true,
        "**/downloads": true,
        "**/sstate-cache": true
    },
    "files.exclude": {
        "build/cache/**": true,
        "build/tmp/**": true,
        "build/sstate-cache/**": true,
        "downloads/**": true,
        "*ctags*":  true,
        "sstate-cache*/**": true,
    },
    "C_Cpp.files.exclude": {
        "build/**": true,
    },
    "python.analysis.exclude": [
        "**/build/**"
    ],
    "search.followSymlinks": false,
    "git-blame.gitWebUrl": "",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        ".repo/**": true,
        "build/**": true,
        "**/build_wsl/**": true,
        "**/downloads/**": true,
        "**/target": true,
        "**/sstate-cache/**": true
    },
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/poky/bitbake/lib",
        "${workspaceFolder}/poky/meta/lib"
    ],
    "python.analysis.extraPaths": [
        "${workspaceFolder}/poky/bitbake/lib",
        "${workspaceFolder}/poky/meta/lib"
    ],
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "copilotcontext.commentSymbol": "#",
        "copilotcontext.languageFileExtension": ".py",
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "off",
        "files.trimTrailingWhitespace": false
    },
    "[shellscript]": {
        "files.eol": "\n",
        "files.trimTrailingWhitespace": false
    },
    "bitbake.commandWrapper": "kas shell -c",
    "bitbake.pathToBuildFolder": "${workspaceFolder}/build",
    "bitbake.pathToEnvScript": "${workspaceFolder}/poky/oe-init-build-env",
    "bitbake.pathToBitbakeFolder": "${workspaceFolder}/poky/bitbake",
    "bitbake.sdkImage": "scailx-image-swupdate-ml",
    "task.saveBeforeRun": "always",
    "bitbake.sshTarget": "root@scailx-thin.local",
    "search.useIgnoreFiles": false
}
